{
  "name": "@kkt/root",
  "private": true,
  "version": "1.3.1",
  "description": "A baseline for server side rendering for your React application.",
  "scripts": {
    "-----ssr plugin-----": "----------",
    "build:plugin:ssr": "lerna exec --scope @kkt/plugin-ssr -- tsbb build",
    "watch:plugin:ssr": "lerna exec --scope @kkt/plugin-ssr -- tsbb watch",
    "clean": "lerna clean && npm run remove && rm -rf ./package-lock.json && rm -rf node_modules && npm run remove:c ",
    "remove": " lerna exec --scope @kkt/* --scope @example/*  -- rm -rf package-lock.json",
    "remove:c": "rm -rf package-lock.json &&  npm run remove:yarn && npm run remove:lib && npm run remove:esm",
    "remove:yarn": "lerna exec --scope @kkt/* --scope @example/*  -- rm -rf yarn.lock",
    "remove:lib": "lerna exec --scope @kkt/*  -- rm -rf ./lib",
    "remove:esm": "lerna exec --scope @kkt/*  -- rm -rf ./esm",
    "tsbb": "tsbb",
    "lerna": "lerna"
  },
  "keywords": [
    "react",
    "react-router",
    "redux",
    "rematch",
    "webpack",
    "express",
    "ssr",
    "next.js"
  ],
  "repository": "https://github.com/kktjs/kkt-ssr.git",
  "author": "Kenny Wong <wowohoo@qq.com> (https://github.com/jaywcjlove)",
  "bugs": {
    "mail": "wowohoo@qq.com",
    "url": "https://github.com/kktjs/kkt-ssr/issues"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,less,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/preset-typescript": "7.15.0",
    "@babel/core": "7.16.7",
    "husky": "7.0.4",
    "lerna": "4.0.0",
    "lint-staged": "12.1.5",
    "prettier": "2.5.1",
    "tsbb": "3.7.0",
    "kkt": "~7.1.5",
    "@babel/plugin-transform-classes": "7.16.7",
    "@babel/plugin-transform-modules-commonjs": "7.16.8"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "example/*"
    ],
    "nohoist": []
  }
}